<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->


  
  <servlet>
    <servlet-name>serverServlet</servlet-name>
    <servlet-class>com.ihsinformatics.is3web.server.ServerServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>serverServlet</servlet-name>
    <url-pattern>/is3web/smssurvey</url-pattern>
  </servlet-mapping>
  
  
   
  <servlet>
    <servlet-name>JspServlet</servlet-name>
    <servlet-class>com.ihsinformatics.is3web.server.JspServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>JspServlet</servlet-name>
    <url-pattern>/JspServlet</url-pattern>
  </servlet-mapping>
  
</web-app>
